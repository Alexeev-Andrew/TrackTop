                        </ul>
                        <div class="tab-content" id="pills-tabContent">
                            <div class="tab-pane fade show active one-tab-ad" id="pills-home" role="tabpanel" aria-labelledby="pills-home-tab"><div class="long_desc"><%-equipment.description.toString().length > 10 ? equipment.description : short_description %></div></div>
                            <div class="tab-pane fade one-tab-ad" id="pills-equipment-usage" role="tabpanel" aria-labelledby="pills-equipment-usage-tab">
                                <% if(type_technic) { %>
                                    <div class="one-ad-usage-row">
                                        <div class="one-ad-usage-label">Техніка</div>
                                        <div class="one-ad-usage-desc-wrap">
                                            <div class="one-ad-usage-desc">
                                                    <%= type_technic%>
                                            </div>
                                        </div>
                                    </div>
                                <%}%>
                                <%if(marks) { %>
                                    <div class="one-ad-usage-row">
                                        <div class="one-ad-usage-label">Марки</div>
                                        <div class="one-ad-usage-desc-wrap">
                                            <div class="one-ad-usage-desc">
                                                    <%= marks%>
                                            </div>
                                        </div>
                                    </div>
                                <%}%>

                                <% if(models) { %>
                                    <div class="one-ad-usage-row">
                                        <div class="one-ad-usage-label">Моделі</div>
                                        <div class="one-ad-usage-desc-wrap">
                                            <% for(let i=0; i< models.length; i++) {%>
                                                <div class="one-ad-usage-desc">
                                                    <%= models[i]%>
                                                </div>
                                            <% } %>
                                        </div>
                                    </div>
                                <%}%>
                            </div>
                            <div class="tab-pane fade one-tab-ad" id="pills-contact" role="tabpanel" aria-labelledby="pills-contact-tab">
