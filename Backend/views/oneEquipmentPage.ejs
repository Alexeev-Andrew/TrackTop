                                    <div class="one-ad-usage-row">
                                        <div class="one-ad-usage-desc-wrap">
                                        <% for(let i=0; i< vendor_code.length; i++) {%>
                                            <div class="one-ad-usage-desc">
                                                <%= vendor_code[i]%>
                                            </div>
                                        <% } %>
                                        </div>
                                    </div>
