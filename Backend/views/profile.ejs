<% include common/header.ejs %>

<div id="main">

    <div class="narrow-wrapper padding-wrapper">

    <h1 class="page-header">Профіль</h1>
    <div class="row no-gutters">

        <div  class="left-col-fixed">
            <img src="http://ssl.gstatic.com/accounts/ui/avatar_2x.png" class="avatar img-thumbnail" alt="avatar" id="my_avatar">
            <h6>Завантажити фото</h6>
            <input
                    type="file"
                    id="photo_input"
                    accept="image/*"
                    class="text-center center-block  file-wrapper">
            <input type="button" >
        </div>

        <div class="col-lg-12 col-xs-12 text-center personal-info profile-fields-container">

            <form class="form-horizontal" role="form" id="form_user_info">
                <div class="row no-gutters">
                    <div class="form-group">
                        <label class="control-label">Ім'я</label>
                        <div class="">
                            <input class="form-control" value="" id="name_value" type="name">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label">Прізвище</label>
                        <div class="">
                            <input class="form-control" value="" id="surname_value" type="surname">
                        </div>
                    </div>
                </div>

                <div class="row no-gutters">
                </div>

                <div class="form-group">
                    <label class=" control-label">Телефон</label>
                    <div class="">
                        <input class="form-control" id="phone_value" value="" type="text">
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label">Місце проживання</label>
                    <div class="">
                        <input class="form-control" value="" id="location_value"  type="text">
                    </div>
                </div>

                <div class="form-group">
                    <label class=" control-label">Пункт Нової Пошти</label>
                    <div class="">
                        <input class="form-control" value="" id="location_post_office_value"type="text">
                    </div>
                </div>

                <div class="form-group">
                    <label class="control-label">Відділення Нової Пошти</label>
                    <div class="">
                        <input class="form-control" value="" id="post_office_number_value" type="text">
                    </div>
                </div>

                <div class="form-group">
                    <label class="control-label">Пароль</label>
                    <div class="">
                        <input class="form-control" value="" id="password_value" type="password">
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label">Підтвердіть пароль</label>
                    <div class="">
                        <input class="form-control" value="" id ="password_confirm_value" type="password">
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label"></label>
                    <div class="">
                        <input class="btn btn-primary" id = "update_user_info" value="Зберегти" type="button">
                        <span></span>
                        <input class="btn btn-danger" value="Скасувати" type="reset" onclick="document.location.href='tracktop.com.ua' ">
                    </div>
                </div>
            </form>
        </div>
    </div>

    </div>
</div>

</div>


<!-- jQuery JavaScript -->
<script type="text/javascript" src="https://code.jquery.com/jquery-2.1.4.min.js"> </script>

<!--Bootsrap JavaScript -->
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js"></script>
<script type="text/javascript"  src="assets/js/profile_main.js"> </script>
<!--font awesome-->
</body>
</html>