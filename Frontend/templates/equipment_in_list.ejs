<div class="oneTechnic">
    <div class="thumbnail  one_equipment technic-card">
        <img class="" src="/images/equipments/<%= equipment.main_photo_location %>">
        <div class="caption captionEquipment">
            <div class="nameEquipment">
                <a href="<%= equipment.url %>">
                <div class="child_name"><%= equipment.name %>
                        <% if(equipment.technic_type=="Комбайни"){ %>
                            до комбайна <%= equipment.technic_mark %> <%= equipment.model %>
                            <div style="height: 1px; width: 1px; overflow: hidden">( <%=equipment.mark_name_ukr%>
                                <% if(equipment.model_ukr){ %><%=equipment.model_ukr%>
                                <% } else {%><%= equipment.model %>  <% } %>) </div>
                        <%}%>
                    </div>
                </a>
            </div>

             <% if(!equipment.vendor_code){ %>
                        <div class="price" style="height:40px;">

                        <% } else {%>
                              <div class="price"> <% } %>
                        <div class="price_equipment"><i>Ціна</i></div> <span class="equipment_info"><%= equipment.price %>
                            <% if(equipment.currency.toString() == "гривня") { %>
                                грн
                            <%}%>

                            <% if(equipment.currency.toString() == "долар") { %>
                                $
                            <%}%>
                            </span>
                        </div>

                        <!--
            <div class="amount"><i>Кількість:</i> <%= equipment.amount %></div>
            <div class="state"><i>Стан:</i> <%= equipment.state %></div>
                        !-->
                         <% if( equipment.vendor_code){ %>

                          <div class="vendor_code"><div class="vendor_code_label"><i>Артикул</i></div> <span class ="equipment_info"><%= equipment.vendor_code %> </span></div>
                           <% } %>

        </div>
    </div>

</div>

</div>